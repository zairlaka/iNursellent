<section id="sign_in_wrap" class="p-0">
<div class="container-fluid reg">
    <div class="row main_row">
        <div class="col-md-12">
            <div class="row">
                <div class="col-md-5 p-0 sign_in_left">
                    <div class="signin-left signup-slider owl-carousel owl-theme">
                        <div class="signup-slide" style="background-image: url('img/N1.jpeg');">
                        </div>
                        <div class="signup-slide" style="background-image: url('img/N2.jpeg');">
                        </div>
                        <div class="signup-slide" style="background-image: url('img/N3.jpeg');">
                        </div>
                        <div class="signup-slide" style="background-image: url('img/N4.jpeg');">
                        </div>
                        <div class="signup-slide" style="background-image: url('img/n7.jpg');">
                        </div>
                        <div class="signup-slide" style="background-image: url('img/n9.jpg');">
                        </div>
                    </div>
                    <div class="signup_overlay">
                        <h2>Please Sign up to continue!</h2>
                        <p>
                            We will verify your account and you will get an activation email from our admin.
                            Please follow instructions in the emaail to complete your profile.
                        </p>
                    </div>
                </div>
                <div class="col-md-7 p-0 sign_in_right">
                    <div class="signin-right">
                        <form class="login-form w-100 h-100" action="" id="signup-form">
                            <div class="step-1 h-100 position-relative sign_up_step">
                                <a href="./index.html" class="close-icon">
                                    <img src="img/close.png" alt="">
                                </a>
                                <div class="input__wrap h-sm-100 ">
                                    <div class="brand-img">
                                    </div>
                                    <h3>Setting Up Your Account</h3>
                                    <p class="step-info">
                                        To be considered for high quality, International RN jobs in the US, UK,
                                        Australia, please set up your account. The setup process will take about
                                        20-25 minutes and you can save your work and come back to it later
                                    </p>
                                    <div class="info_from_user">
                                        <div class="info_item align-items-center">
                                          <%= image_tag "img/one.png" %>
                                            <div class="info-disc">
                                                <p class="info-title mt-1">Your personal infomation</p>
                                            </div>
                                        </div>
                                        <div class="info_item align-items-center">
                                          <%= image_tag "img/two.png" %>
                                            <div class="info-disc">
                                                <p class="info-title mt-1">Professional Experience</p>
                                            </div>
                                        </div>
                                        <div class="info_item align-items-center">
                                          <%= image_tag "img/three.png" %>
                                            <div class="info-disc">
                                                <p class="info-title mt-1">Specialty Areas in Nursing</p>
                                            </div>
                                        </div>
                                        <div class="info_item align-items-center">
                                          <%= image_tag "img/four.png" %>
                                            <img src="./frontend/img/four.png" alt="">
                                            <div class="info-disc">
                                                <p class="info-title mt-1">Nursing Educational Details</p>
                                            </div>
                                        </div>
                                        <div class="info_item align-items-center">
                                          <%= image_tag "img/five.png" %>
                                            <div class="info-disc">
                                                <p class="info-title mt-1">Location Preferences, Passport
                                                    Details
                                                    &amp;
                                                    Work
                                                    Permit</p>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <button class="theme_btn w-100 rounded border-0 py-3 continue">Next</button>
                            </div>
                            <div
                                class="step-2 position-relative h-100 h-96 back-icon signin_step sign_up_email">
                                <div class="close-icon Back">
                                  <%= image_tag "img/left-arrow.png" %>
                                </div>
                                <div class="input__wrap h-100 h-98">
                                    <h3>Whats your Email</h3>
                                    <div class="form-floating mb-3 current-user-email">
                                        <input type="email"
                                            class="form-control border-0 ps-0 rounded-0 border-bottom"
                                            id="floatingInput" placeholder="name@example.com">
                                        <label for="floatingInput">Email address</label>
                                    </div>
                                </div>
                                <button class="theme_btn w-100 rounded border-0 py-3 continue user-sign-up">Continue</button>
                            </div>
                            
                            <div class="step-3 position-relative back-icon h-100 signin_step sign_up_code">
                                <div class="close-icon Back">
                                  <%= image_tag "img/left-arrow.png" %>
                                </div>
                                <div class="input__wrap h-100 h-96">
                                    <h3 class="">Enter 4 digit code sent to your email</h3>
                                    <div class="d-flex gap-4 mt-5 pe-sm-5">
                                        <div class="mb-3">
                                            <input type="number"
                                                class="form-control rounded-5 text-center bg-light border-bottom"
                                                id="floatingInput" placeholder="">
                                        </div>
                                        <div class="mb-3">
                                            <input type="number"
                                                class="form-control rounded-5 text-center bg-light border-bottom"
                                                id="floatingInput" placeholder="">
                                        </div>
                                        <div class="mb-3">
                                            <input type="number"
                                                class="form-control rounded-5 text-center bg-light border-bottom"
                                                id="floatingInput" placeholder="">
                                        </div>
                                        <div class="mb-3">
                                            <input type="number"
                                                class="form-control rounded-5 text-center bg-light border-bottom"
                                                id="floatingInput" placeholder="">
                                        </div>
                                    </div>
                                </div>
                                <button class="theme_btn w-100 rounded border-0 py-3 continue">Continue</button>
                            </div>
                            
                            <%= form_for User.new ,url: "#",method: :put, multipart: true, html: {class: ""} do  |f| %>
                            <div class="main-sign-up-form">
                              <div class="step-4 position-relative back-icon h-100 signin_step sign_up_Pi">
                                  <div class="close-icon Back">
                                    <%= image_tag "img/left-arrow.png" %>
                                  </div>
                                  <div class="input__wrap h-100 h-96">
                                    <%= f.fields_for :detail do |d| %>
                                      <h3>Personal Information</h3>
                                      <div class="d-flex gap-2">
                                          <div class="form-floating mb-2 w-50">
                                            <%= d.text_field :address, class:"form-control border-0 ps-0 rounded-0 border-bottom", id: "floatingInput", placeholder:"" %>
                                              <label for="floatingInput" class="ps-0">First Name</label>
                                          </div>
                                          <div class="form-floating mb-2 w-50">
                                              <input type="text"
                                                  class="form-control border-0 ps-0 rounded-0 border-bottom"
                                                  id="floatingInput" placeholder="">
                                              <label for="floatingInput" class="ps-0">Last Name</label>
                                          </div>
                                      </div>
                                      <div class="form-floating mb-2 w-50">
                                        <%= d.text_field :address, class:"form-control border-0 ps-0 rounded-0 border-bottom", id: "floatingInput", placeholder:"" %>
                                          <label for="floatingInput" class="ps-0">Address</label>
                                          </div>
                                      <div class="form-floating mb-2">
                                          <input type="email"
                                              class="form-control border-0 ps-0 rounded-0 border-bottom"
                                              id="floatingInput" placeholder="">
                                          <label for="floatingInput" class="ps-0">Email</label>
                                      </div>
                                      <div class="d-flex gap-2">
                                          <div class="form-floating mb-2 w-50">
                                              <input type="text"
                                                  class="form-control border-0 ps-0 rounded-0 border-bottom"
                                                  id="floatingInput" placeholder="">
                                              <label for="floatingInput" class="ps-0">Mobile number</label>
                                          </div>
                                          <div class="form-floating mb-2 w-50">
                                              <input type="number"
                                                  class="form-control border-0 ps-0 rounded-0 border-bottom"
                                                  id="floatingInput" placeholder="">
                                              <label for="floatingInput" class="ps-0">Zip Code</label>
                                          </div>
                                      </div>
                                      <h5 class="my-3">Contact preference</h5>
                                      <label for="floatingInput" class="ps-0">How would you prefer to be
                                          contacted (select one or more)?</label>
                                      <div class="py-3">
                                          <div class="checkbox-wrapper-13">
                                              <input id="c1-13" class="p-0" type="checkbox">
                                              <label for="c1-13">Text</label>
                                          </div>
                                          <div class="checkbox-wrapper-13">
                                              <input id="c1-13" class="p-0" type="checkbox">
                                              <label for="c1-13">Whatsapp</label>
                                          </div>
                                          <div class="checkbox-wrapper-13">
                                              <input id="c1-13" class="p-0" type="checkbox">
                                              <label for="c1-13">Email</label>
                                          </div>
                                      </div>
                                    <% end%>
                                  </div>
                                  <button class="theme_btn w-100 rounded border-0 py-3 continue">Continue</button>
                              </div>
                              <div class="step-5 position-relative back-icon h-100 h-98 signin_step sign_up_step_5">
                                  <div class="close-icon Back">
                                    <%= image_tag "img/left-arrow.png" %>
                                  </div>
                                  <div class="input__wrap h-100 h-98">
                                      <div class="brand-img">
                                        <%= image_tag "img/company-logo.png" %>
                                      </div>
                                      <h3>One down, four to go</h2>
                                          <p class="step-info">
                                              You are going to need your license details for this step. Go grab
                                              that
                                              now
                                              if you don,t have it handy. W,ll wait.
                                          </p>
                                          <div class="info_from_user">
                                              <div class="info_item align-items-center">
                                                <%= image_tag "img/check.svg" %>
                                                  <div class="info-disc">
                                                      <p class="info-title mt-1">Your personal infomation</p>
                                                  </div>
                                              </div>
                                              <div class="info_item align-items-center">
                                                <%= image_tag "img/two.png" %>
                                                  <div class="info-disc">
                                                      <p class="info-title mt-1">Professional Experience</p>
                                                  </div>
                                              </div>
                                              <div class="info_item align-items-center">
                                                <%= image_tag "img/three.png" %>
                                                  <div class="info-disc">
                                                      <p class="info-title mt-1">Specialty Areas in Nursing</p>
                                                  </div>
                                              </div>
                                              <div class="info_item align-items-center">
                                                <%= image_tag "img/four.png" %>
                                                  <div class="info-disc">
                                                      <p class="info-title mt-1">Nursing Educational Details</p>
                                                  </div>
                                              </div>
                                              <div class="info_item align-items-center">
                                                  <%= image_tag "img/five.png" %>
                                                  <div class="info-disc">
                                                      <p class="info-title mt-1">Location Preferences, Passport
                                                          Details &
                                                          Work
                                                          Permit</p>
                                                  </div>
                                              </div>
                                          </div>
                                  </div>
                                  <button
                                      class="theme_btn w-100 rounded border-0 py-3 mb-2 continue">Next</button>
                              </div>

                              <div class="step-6 position-relative back-icon h-100 signin_step sign_up_Qi">
                                  <div class="close-icon Back">
                                    <%= image_tag "img/left-arrow.png" %>
                                  </div>
                                  <div class="input__wrap h-100 h-96 pt-sm-0 pt-4">
                                      <h3 class="">Qualifications Information</h3>
                                      <div class="">
                                          <div class="form-floating mb-2 w-100">
                                              <input type="text"
                                                  class="form-control border-0 ps-0 rounded-0 border-bottom"
                                                  id="floatingInput" placeholder="">
                                              <label for="floatingInput" class="ps-0">Degree</label>
                                          </div>
                                          <div class="form-floating mb-2 w-100">
                                              <input type="text"
                                                  class="form-control border-0 ps-0 rounded-0 border-bottom"
                                                  id="floatingInput" placeholder="">
                                              <label for="floatingInput" class="ps-0">Name of College</label>
                                          </div>
                                      </div>
                                      <div class="">
                                          <div class="form-floating mb-2 w-100">
                                              <input type="text"
                                                  class="form-control border-0 ps-0 rounded-0 border-bottom"
                                                  id="floatingInput" placeholder="">
                                              <label for="floatingInput" class="ps-0">City of College</label>
                                          </div>
                                          <div class="form-floating mb-2 w-100">
                                              <input type="url"
                                                  class="form-control border-0 ps-0 rounded-0 border-bottom"
                                                  id="floatingInput" placeholder="">
                                              <label for="floatingInput" class="ps-0">Website of college
                                                  (optional)</label>
                                          </div>
                                      </div>

                                      <div class="">
                                          <div class="form-floating mb-2 w-100">
                                              <input type="date"
                                                  class="form-control border-0 ps-0 rounded-0 border-bottom"
                                                  id="floatingInput" placeholder="">
                                              <label for="floatingInput" class="ps-0">Date of Degree</label>
                                          </div>
                                      </div>

                                  </div>
                                  <button class="theme_btn w-100 rounded border-0 py-3 continue">Continue</button>
                              </div>
                              <div class="step-6 position-relative back-icon h-100 signin_step reference_screen">
                                  <div class="close-icon Back">
                                      <%= image_tag "img/left-arrow.png" %>
                                  </div>
                                  <div class="input__wrap h-100 h-96 pt-sm-0 pt-4">
                                      <h3 class="my-3">Professional Nursing/Clinical References</h3>
                                      <div class="d-flex gap-2 flex-colunm flex-sm-row input_group_wrap">
                                          <div class="form-floating mb-2 w-50">
                                              <input type="text"
                                                  class="form-control border-0 ps-0 rounded-0 border-bottom"
                                                  id="floatingInput" placeholder="">
                                              <label for="floatingInput" class="ps-0">Name of your
                                                  reference</label>
                                          </div>
                                          <div class="form-floating mb-2 w-50">
                                              <input type="number"
                                                  class="form-control border-0 ps-0 rounded-0 border-bottom"
                                                  id="floatingInput" placeholder="">
                                              <label for="floatingInput" class="ps-0">Contact of your
                                                  reference</label>
                                          </div>
                                      </div>
                                      <div class="form-floating mb-2 w-100">
                                          <input type="email"
                                              class="form-control border-0 ps-0 rounded-0 border-bottom"
                                              id="floatingInput" placeholder="">
                                          <label for="floatingInput" class="ps-0">Email of your
                                              reference</label>
                                      </div>
                                      <div class="d-flex gap-2 flex-colunm flex-sm-row input_group_wrap">
                                          <div class="form-floating mb-2 w-50">
                                              <input type="text"
                                                  class="form-control border-0 ps-0 rounded-0 border-bottom"
                                                  id="floatingInput" placeholder="">
                                              <label for="floatingInput" class="ps-0">Name of your
                                                  reference</label>
                                          </div>
                                          <div class="form-floating mb-2 w-50">
                                              <input type="number"
                                                  class="form-control border-0 ps-0 rounded-0 border-bottom"
                                                  id="floatingInput" placeholder="">
                                              <label for="floatingInput" class="ps-0">Contact of your
                                                  reference</label>
                                          </div>
                                      </div>

                                      <div class="form-floating mb-2 w-100">
                                          <input type="email"
                                              class="form-control border-0 ps-0 rounded-0 border-bottom"
                                              id="floatingInput" placeholder="">
                                          <label for="floatingInput" class="ps-0">Email of your
                                              reference</label>
                                      </div>
                                      <div class="d-flex gap-2 flex-colunm flex-sm-row input_group_wrap">
                                          <div class="form-floating mb-2 w-50">
                                              <input type="text"
                                                  class="form-control border-0 ps-0 rounded-0 border-bottom"
                                                  id="floatingInput" placeholder="">
                                              <label for="floatingInput" class="ps-0">Name of your
                                                  reference</label>
                                          </div>
                                          <div class="form-floating mb-2 w-50">
                                              <input type="number"
                                                  class="form-control border-0 ps-0 rounded-0 border-bottom"
                                                  id="floatingInput" placeholder="">
                                              <label for="floatingInput" class="ps-0">Contact of your
                                                  reference</label>
                                          </div>
                                      </div>

                                      <div class="form-floating mb-2 w-100">
                                          <input type="email"
                                              class="form-control border-0 ps-0 rounded-0 border-bottom"
                                              id="floatingInput" placeholder="">
                                          <label for="floatingInput" class="ps-0">Email of your
                                              reference</label>
                                      </div>
                                  </div>
                                  <button class="theme_btn w-100 rounded border-0 py-3 continue">Continue</button>
                              </div>
                              <div class="step-7 position-relative back-icon h-100 signin_step sign_up_hopitals">
                                  <div class="close-icon Back">
                                      <%= image_tag "img/left-arrow.png" %>
                                  </div>
                                  <div class="input__wrap justify-content-start worked-hospitls">
                                      <h3 class="mb-4">Please enter information about various
                                          hospitals where you have worked as an RN</h3>
                                      <div id="all-hopitals-data" class="all-hopitals-data">
                                          <div class="hospitals_worked_wrap mb-5">
                                              <div class="my-2 input_for_hospital flex-colunm">
